{
  "name": "eyeem-components",
  "version": "0.1.19",
  "description": "Shareable React Components for Frontend Projects at EyeEm",
  "main": "dist/index.js",
  "skpm": {
    "main": "button.sketchplugin",
    "manifest": "manifest.json"
  },
  "scripts": {
    "watch": "node_modules/.bin/webpack --config webpack.config.js --watch",
    "build": "node_modules/.bin/webpack --config webpack.config.js",
    "test": "echo 'no tests yet :(';",
    "prepublish": "npm run build",
    "release": "np",
    "build-sketch": "skpm-build",
    "watch-sketch": "skpm-build --watch",
    "render-sketch": "skpm-build --watch --run",
    "render-sketch:once": "skpm-build --run",
    "postinstall": "npm run build && skpm-link"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eyeem/frontend-components.git"
  },
  "author": "EyeEm Mobile GmbH (eyeem.com)",
  "bugs": {
    "url": "https://github.com/eyeem/frontend-components/issues"
  },
  "homepage": "https://github.com/eyeem/frontend-components#readme",
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-primitives": "^0.5.0",
    "react-sketchapp": "^1.0.0",
    "react-test-renderer": "^16.2.0",
    "styled-normalize": "^2.2.1"
  },
  "peerDependencies": {
    "lodash": "^4.17.4",
    "react": "^15.0.0-0 || ^16.0.0-0",
    "react-create-component-from-tag-prop": "^1.3.1",
    "styled-components": "^2.2.1"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@skpm/builder": "^0.4.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "lodash": "^4.17.4",
    "np": "^2.16.0",
    "react-create-component-from-tag-prop": "^1.3.1",
    "styled-components": "^3.1.6",
    "webpack": "^3.6.0"
  }
}
